{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./",
      "typeRoots": ["./node_modules/@types", "./src/types"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strictNullChecks": false,
      "types": [
        "node"
      ]
    },
    "include": [
      "**/*",
      "src",
      "src/types"
    ],
    "exclude": [
      "node_modules",
      "dist"
    ],
  
  
    // "types": ["node", "jest"]
  
  }


